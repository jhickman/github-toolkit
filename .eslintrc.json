{
  "extends": ["eslint:recommended", "google"],
  "parserOptions": {
    "ecmaVersion": 5
  },
  "env": {
    "browser": true,
    "jquery": true
  },
  "globals": {
    "define": false,
    "require": false,
    "_": false,
    "chrome": false
  },
  "rules": {
    "no-var": "off",
    "prefer-rest-params": "off"
  }
}
